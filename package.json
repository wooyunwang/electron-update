{
  "name": "electron-quick-start",
  "version": "1.0.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "mac": "electron-packager ./ --platform=mas --icon=icon/icon.icns --overwrite",
    "win": "electron-packager ./ --platform=win32 --icon=icon/icon.ico --overwrite",
    "icon": "./node_modules/.bin/electron-icon-maker --input=icon/logo.png --output=icon"
  },
  "repository": "git@github.com:naihe138/electron-update.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.7",
    "electron-icon-maker": "0.0.4",
    "electron-packager": "^13.1.1"
  },
  "mac": {
    "icon": "icon/icon.icns"
  },
  "win": {
    "icon": "icon/icon.ico"
  },
  "dependencies": {
    "update-electron-app": "^1.3.0"
  }
}
